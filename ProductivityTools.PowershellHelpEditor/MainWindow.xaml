<Window x:Class="ProductivityTools.PowershellHelpEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProductivityTools.PowershellHelpEditor"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Create about help" HorizontalAlignment="Left" Command="{Binding OpenAboutWindowCommand}" VerticalAlignment="Top" Margin="154,0,0,0"/>
        <Button Content="Open directory" HorizontalAlignment="Left" Command="{Binding OpeFileDialogCommand}" Margin="70,20,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
